{
  "name": "selenodium-grid",
  "description": "A Selenium Grid reimplementation in NodeJS",
  "tags": [
    "selenium",
    "grid"
  ],
  "authors": [
    {
      "name": "Sergey Belov",
      "email": "peimei@ya.ru"
    },
    {
    "name": "Testingbot",
    "email": "info@testingbot.com"
    }
  ],
  "bin": {
    "selenodium-grid": "bin/selenodium-grid"
  },
  "version": "0.1.0",
  "dependencies": {
    "argparse": "~0.1.10",
    "colors": "~0.6.0-1",
    "extend": "^2.0.0",
    "inherit": "^2.2.2",
    "lodash": "^3.3.1",
    "luster": "^0.5.3",
    "luster-guard": "^0.4.0",
    "q": "^1.1.2",
    "q-io": "^1.11.6",
    "qs": "^2.3.3",
    "server-destroy": "^1.0.0",
    "tracer": "*"
  },
  "devDependencies": {
    "coveralls": "^2.11.2",
    "mocha": "^2.1.0",
    "must": "^0.12.0",
    "should": "^5.0.0",
    "supertest": "^0.15.0",
    "unit-coverage": "^3.4.0"
  },
  "directories": {
    "lib": "./lib"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/selenodium/selenodium-grid.git"
  },
  "bugs": {
    "url": "https://github.com/selenodium/selenodium-grid/issues"
  },
  "engines": {
    "node": ">= 0.10"
  },
  "unit-coverage": {
    "common": [
      "--sources",
      "lib/**/*.js",
      "--sources",
      "server.js",
      "--sources",
      "index.js",
      "--exclude",
      "node_modules/**",
      "--tests",
      "test/**/*.js",
      "--additional",
      "server.js",
      "--additional",
      "index.js",
      "--additional",
      "lib",
      "--additional",
      "test"
    ]
  },
  "scripts": {
    "start": "luster",
    "test": "mocha",
    "coverage": "unit-coverage run --quiet --profile common",
    "coverage-html": "npm run coverage -- --reporter html --output coverage.html",
    "travis": "npm test && npm run coverage -- --reporter lcov | coveralls"
  }
}
